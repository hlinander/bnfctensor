n in SO(3) {
    dim n delta;
    symmetric delta _a _b;
    delta _a ^a = n;

    dim n epsilon;
    anti_symmetric epsilon;
    epsilon _a1 _a2 ... _an epsilon ^b1 ^b2 ... ^bn
        = delta [_a1 ^[b1 delta _a2 ^b2  delta _an] delta^bn];
}

n \in SO(3, 1) {
    invariants {
        symmetric \delta _a _b
        \delta _a ^a = n
        dim n \epsilon
        anti_symmetric \epsilon 
        \epsilon _a1 _a2 ... _an \epsilon ^b1 ^b2 ... ^bn 
          = (-1)^(n) \delta [_a1 ^[b1 \delta _a2 ^b2 ... \delta_an] \delta^bn];
    }    
}

